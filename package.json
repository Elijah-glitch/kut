    "express": "^4.18.2",
    "express-timeout-handler": "^2.2.2",
    "express-useragent": "^1.0.15",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "mongoose": "^6.8.2",
    "morgan": "^1.10.0",
    "open-graph-scraper": "^5.0.2",
    "pinia": "^2.0.28",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "qrcode": "^1.5.1",
    "vue-tiny-validate": "^0.2.4",
    "winston": "^3.8.2"
