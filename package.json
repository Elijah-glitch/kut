    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.18",
    "@vueuse/core": "^9.9.0",
    "@vueuse/integrations": "^9.9.0",
    "@vueuse/nuxt": "^9.9.0",
    "eslint": "^8.30.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "^3.0.0",
    "nuxt-icon": "^0.1.8",
    "prettier": "^2.8.1",
    "simple-git-hooks": "^2.8.1",
