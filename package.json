{
  "name": "kut",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "start": "node -C production .output/server/index.mjs",
    "lint": "eslint . --ext \".ts,.js,.vue,.json\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,json,vue}\"",
    "postinstall": "npx simple-git-hooks && nuxt prepare"
